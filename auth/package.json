    "pino-pretty": "^10.2.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/eslint": "8.44.2",
    "@typescript-eslint/eslint-plugin": "6.6.0",
    "@typescript-eslint/parser": "6.6.0",
    "bun-types": "latest",
    "dotenv": "^16.3.1",
